{
  "manifest_version": 2,
  "name": "MultiDownloader",
  "description": "Batch download and rename images from URL's",
  "version": "0.1",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "permissions": [
    "downloads", "tabs", "<all_urls>"
  ]
}
